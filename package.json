{
  "name": "cachinator",
  "version": "1.0.0",
  "description": "Rate limiting + caching middleware for Express with Redis and memory fallback",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.json",
    "dev": "ts-node-dev --respawn --transpile-only examples/express/index.ts",
    "test": "jest --runInBand",
    "test:monitoring": "node examples/test-monitoring.js",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "format": "prettier --write .",
    "prepare": "npm run build",
    "commitlint": "commitlint --edit",
    "semantic-release": "semantic-release"
  },
  "engines": {
    "node": ">=16"
  },
  "keywords": [
    "express",
    "rate-limit",
    "caching",
    "redis",
    "middleware",
    "typescript"
  ],
  "author": "Yash gupta",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yashgupta18/cachinator.git"
  },
  "bugs": {
    "url": "https://github.com/yashgupta18/cachinator/issues"
  },
  "homepage": "https://github.com/yashgupta18/cachinator#readme",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "express": ">=4.0.0",
    "ioredis": ">=5.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^11.1.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.6",
    "@semantic-release/npm": "^10.0.4",
    "@semantic-release/release-notes-generator": "^12.1.0",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "express": "^5.1.0",
    "ioredis": "^5.7.0",
    "jest": "^30.1.3",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "semantic-release": "^22.0.12",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2"
  }
}
