{
  "name": "rate-limit-pkg",
  "version": "0.1.0",
  "description": "Rate limiting + caching middleware for Express with Redis and memory fallback",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.json",
    "dev": "ts-node-dev --respawn --transpile-only examples/express/index.ts",
    "test": "jest --runInBand",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "format": "prettier --write ."
  },
  "keywords": [
    "express",
    "rate-limit",
    "caching",
    "redis",
    "middleware",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^5.1.0",
    "ioredis": "^5.7.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.1.3",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2"
  }
}
